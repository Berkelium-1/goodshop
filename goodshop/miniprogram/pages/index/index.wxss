/* 轮播图 */
.home-swiper swiper{
    height: 340rpx;
}
/* 导航 */
.home-cates{
    width: 100%;
    display: flex;
}
.home-cates navigator{
    flex: 1;
}
.home-cates navigator{
    width: 100%;
    padding: 30rpx;
}
/* 楼层 */
.home-floot{
    /* margin: 10rpx auto; */
}
.home-floot .floot-item .item-title{
    padding: 10rpx;
    color: #ff859e;
    font-size: 36rpx;
    font-weight: bolder;
    line-height: 72rpx;
}

.home-floot .floot-item .item-list{
    width: 100%;
    display: grid;
    grid-template-rows: repeat(2, 1fr);
    grid-template-columns: repeat(3, 1fr);
}

.home-floot .floot-item .item-list navigator{
    padding-right: 10rpx;
}

.home-floot .floot-item .item-list navigator:first-child{
    padding-left: 10rpx;
    grid-row: 1/span 2;
}

.home-floot .floot-item .item-list navigator:nth-of-type(n+4){
    margin-top: 10rpx;
}

.home-floot .floot-item .item-list navigator image{
    display: block;
    width: 100%;
    height: 100%;
}