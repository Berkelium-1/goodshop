/* components/Tabs/Tabs.wxss */
.tabs .tabs-title{
  display: flex;
}
.tabs .tabs-title .tabs-item{
  flex: 1;
  padding: 15rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs .tabs-title .tabs-item.active{
  color: var(--themeColor);
  border-bottom: 3px solid currentColor;
}